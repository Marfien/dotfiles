[alias]
	ec = commit --allow-empty -m
	cm = commit -m
	sconf = config --system
	gconf = config --global
	lconf = config --local
	again = commit -C HEAD --amend --no-edit
	rollback = reset --hard
	push-skip = push -o ci.skip
	pfush = push --force-with-lease
	s = status
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[core]
	autocrlf = input
